<% if user_signed_in? %>
  <div class="search_form" style="margin-top:40px;">
    <%= form_with url: user_search_path(user_id), local: false, method: :get do |f| %>
      <%= f.date_field :date %>
      <%= f.submit "検索", class: "btn btn-outline-primary" %>
    <% end %>
  </div>
  <% unless bookscount.nil? %>
    <h4>検索結果</h4>
    <%= bookscount %>
  <% end %>
<% end %>